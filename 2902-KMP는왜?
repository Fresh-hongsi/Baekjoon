#include <iostream>
#include <algorithm>
#include <string>
#include <vector>
using namespace std;

int main(void)
{
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
	cout.tie(nullptr);

	string input;
	cin >> input;

	vector<char> arr;


	for (int i = 0; i < input.length(); i++)
	{
		if (input[i] >= 65 && input[i] <= 90)
		{
			arr.push_back(input[i]);
		}
	}

	for (int i = 0; i < arr.size(); i++)
	{
		cout << arr[i];
	}

	return 0;
}
